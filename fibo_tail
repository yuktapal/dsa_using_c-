#include <stdio.h>
 

int fib(int n,int a,int b)
{
    if (n == 0)
    return a;
    else{
    
return fib(n - 1,b, a + b);}

}
int fibo(int n){
return fib(n,0,1);
}
int main()
{
   int n,i ;
   printf("Enter the term ");
   scanf("%d",&n);
   for(i=0;i<n;i++)
   printf("%d ",fibo(i));
  
    return 0;
}
